    "cross-env": "^5.0.5"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-medium#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "license": "MIT",
  "main": "index.js",
