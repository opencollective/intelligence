    "jasmine-jquery": "^2.1.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-verbose-reporter": "0.0.6",
    "prettier": "^1.12.1",
    "protractor": "^5.3.2",
