    "@types/google.analytics": "^0.0.39",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.7",
    "@types/jquery": "^3.3.2",
    "@types/jszip": "^3.1.3",
    "@types/lodash": "^4.14.108",
