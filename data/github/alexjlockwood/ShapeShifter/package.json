  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --browser ChromeHeadless",
    "test-once": "ng test --browser ChromeHeadless --single-run --code-coverage",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "deploy": "./scripts/deploy.sh",
    "deploy-beta": "./scripts/deploy-beta.sh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "bundle-report": "ng build --prod --target=production --environment=prod --aot --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
