{
  "name": "ts-express-decorators",
  "version": "4.18.0",
  "description": "A TypeScript Framework on top of Express",
  "main": "./dist/lib/index.js",
  "typings": "./dist/lib/index.d.ts",
  "scripts": {
    "postinstall": "gulp mount:packages",
    "clean": "gulp clean:workspace",
    "tslint": "tslint --project tsconfig.json",
    "test": "npm run clean && npm run tsc && npm run tslint && cross-env NODE_ENV=test nyc --reporter=html --reporter=text _mocha --recursive",
    "coveralls:travis": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "build": "gulp build",
    "build:release": "gulp build && gulp doc",
    "publish:release": "gulp publish:packages",
    "doc:serve": "gulp build:doc && docsify serve ./docs --port 3001",
    "doc:build": "gulp build:doc",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Romakita/ts-express-decorators.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "decorators",
    "router",
    "controllers",
    "services",
    "middlewares",
    "models",
    "mvc",
    "injection",
    "ioc",
    "di",
    "api",
    "swagger",
    "mongoose",
    "ajv",
    "socketio",
    "ES2015",
    "ES6",
    "server",
    "rest",
    "validation",
    "multer"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Romakita/ts-express-decorators/issues"
  },
  "homepage": "https://romakita.github.io/ts-express-decorators/#/",
  "dependencies": {
    "@types/json-schema": "^6.0.0",
    "globby": "^8.0.1",
    "reflect-metadata": "^0.1.12",
    "ts-httpexceptions": "^3.0.2",
    "ts-log-debug": "^4.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "7.1.0",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/globby": "^6.1.0",
    "@types/mocha": "^5.2.0",
    "@types/mongoose": "^5.0.6",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.32",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^4.3.3",
    "@types/sinon-chai": "^2.7.27",
    "@types/socket.io": "^1.4.31",
    "@types/superagent": "^3.5.3",
    "@types/supertest": "^2.0.0",
    "@types/swagger-schema-official": "^2.0.9",
    "ajv": "^6.5.0",
    "body-parser": "^1.15.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.1",
    "compression": "^1.6.1",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "docsify-cli": "^4.1.9",
    "docsify-ts-api": "^2.0.0",
    "ejs": "^2.5.7",
    "ejs-lint": "^0.3.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-exec": "^3.0.1",
    "gulp-json-editor": "^2.2.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.1",
    "html-minifier": "^3.5.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "method-override": "^2.3.10",
    "mocha": "^5.1.1",
    "mongoose": "^5.0.9",
    "multer": "^1.3.0",
    "nyc": "^11.7.3",
    "prettier": "^1.12.1",
    "proxyquire": "^2.0.1",
    "read-package-json": "^2.0.12",
    "require-dir": "^1.0.0",
    "semantic-release": "^7.0.2",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.1.0",
    "supertest": "^3.0.0",
    "swagger-ui-dist": "^3.13.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "typescript": "^2.5.0"
  },
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "release": {
    "debug": false,
    "branch": "production"
  },
  "gflow": {
    "master": "master",
    "production": "production"
  },
  "docsifyApi": {
    "scope": "@tsed",
    "modules": {
      "core": "core",
      "common": {
        "config": "common/config",
        "converters": "common/converters",
        "di": "common/di",
        "filters": "common/filters",
        "jsonschema": "common/jsonschema",
        "mvc": "common/mvc",
        "interceptors": "common/interceptors",
        "server": "common/server"
      },
      "ajv": "ajv",
      "mongoose": "mongoose",
      "multipartfiles": "multipartfiles",
      "servestatic": "servestatic",
      "socketio": "socketio",
      "swagger": "swagger",
      "testing": "testing"
    }
  }
}