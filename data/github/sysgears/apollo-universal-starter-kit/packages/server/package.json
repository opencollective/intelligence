  "main": "src",
  "engines": {
    "node": ">= 6.11.1",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production spin build",
    "clean": "rimraf build .tmp",
    "start": "node --harmony build",
    "tests": "cross-env NODE_ENV=test PORT=7070 spin test -t 10000 --full-trace --exit \"src/**/*.spec.js\"",
    "tests:watch": "cross-env NODE_ENV=test PORT=7070 spin test -t 10000 --full-trace --watch \"src/**/*.spec.js\"",
    "test": "yarn tests && yarn lint",
    "lint": "eslint --fix --ext js --ext jsx --ext json src config",
    "seed": "yarn migrate && knex seed:run",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "watch": "spin watch",
    "heroku-postbuild": "yarn build && yarn seed",
    "cli": "node ../../tools/cli",
    "stripe:setup": "node src/server/modules/subscription/cli/stripeSetup"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ],
    "*.graphql": [
      "prettier --parser graphql --write",
      "git add"
    ]
  },
