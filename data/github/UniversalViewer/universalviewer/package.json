    "exjs": "BSick7/exjs#0.5.1",
    "hls.js": "0.9.1",
    "http-status-codes": "edsilv/http-status-codes#v0.0.8",
    "iiif-av-component": "iiif-commons/iiif-av-component#v0.0.38",
    "iiif-gallery-component": "iiif-commons/iiif-gallery-component#v1.1.4",
    "iiif-metadata-component": "iiif-commons/iiif-metadata-component#v1.1.7",
    "iiif-tree-component": "iiif-commons/iiif-tree-component#v1.1.6",
    "jquery": "2.2.4",
    "jquery-plugins": "edsilv/jquery-plugins#v1.0.0",
    "jquery-tiny-pubsub": "edsilv/jquery-tiny-pubsub#v0.7.2",
    "jquery-ui-dist": "^1.12.1",
    "jsviews": "0.9.83",
    "key-codes": "edsilv/key-codes#v0.0.7",
    "manifesto.js": "2.2.21",
    "@iiif/manifold": "1.2.24",
    "mediaelement": "4.0.2",
    "opencollective": "1.0.3",
    "openseadragon": "2.2.1",
    "pdfjs-dist": "2.0.161",
    "requirejs": "2.2.0",
    "three": "0.91.0",
    "utils": "edsilv/utils#v0.2.1",
    "uv-cy-gb-theme": "UniversalViewer/uv-cy-gb-theme#v1.1.1",
    "uv-en-gb-theme": "UniversalViewer/uv-en-gb-theme#v1.1.1",
    "virtex3d": "0.3.14"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/universalviewer",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    },
    "preset": "jest-puppeteer",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
