    "hooker": "0.2.3",
    "http-server": "^0.11.1",
    "jest": "^22.4.3",
    "jest-puppeteer": "^2.2.0",
    "json-schema-generator": "edsilv/json-schema-generator",
    "less": "2.4.0",
    "lodash": "4.17.4",
    "ncp": "2.0.0",
    "puppeteer": "^1.2.0",
    "ts-jest": "^22.4.2",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@iiif/manifold": "1.2.24",
    "@types/modernizr": "3.2.29",
